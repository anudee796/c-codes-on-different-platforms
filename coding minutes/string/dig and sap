#include<iostream>
using namespace std;
int main(){
    char ch[20];
    cin.getline(ch,20,'\n');
    int s=0,d=0;
    for(int i=0;ch[i]!='\0';i++){
        if(ch[i]<='9'&&ch[i]>='0'){
            d++;
        }
        if(ch[i]==' '){
            s++;
        }
    }
    cout<<"spaces : "<<s<<"digits : "<< d;
}
